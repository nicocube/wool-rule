{
  "name": "wool-rule",
  "version": "1.5.0",
  "description": "Lib for Rules in Wool",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "tape test/**/*.spec.js |tap-summary --no-progress",
    "autotest": "nodemon -e js,yml -x 'tape test/**/*.spec.js |tap-summary --no-progress'",
    "autolinttest": "nodemon -e js,yml -x 'npm run lint && tape test/**/*.spec.js |tap-summary --no-progress'",
    "coverage": "nyc tape test/**/*.spec.js && nyc report --reporter=html",
    "lint": "eslint index.js lib/ test/",
    "autolint": "nodemon -x eslint index.js lib/ test/",
    "link-all": "for u in $(ls -1d node_modules/wool-* | xargs basename -a); do npm link $u; done;",
    "preversion": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicocube/wool-rule.git"
  },
  "keywords": [
    "rule"
  ],
  "author": "Nicolas Lochet <nicolas.lochet@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/nicocube/wool-rule/issues"
  },
  "homepage": "https://github.com/nicocube/wool-rule#readme",
  "devDependencies": {
    "eslint": "^5.5.0",
    "nodemon": "^1.18.4",
    "nyc": "^13.0.1",
    "tap-summary": "^4.0.0",
    "tape": "^4.9.1"
  },
  "dependencies": {
    "wool-model": "^1.2.3",
    "wool-store": "^1.2.2",
    "wool-validate": "^1.0.0"
  }
}
